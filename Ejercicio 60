' Gambas module file

Public Sub Main()
  Dim i, j, n As Short
  
  Print "Ingrese un numero positivo"
  Input n
  
  If n > 0 Then
    For i = 1 To n
      For j = 1 To n
        If i == j Then
          Print "0";;
        Else
          Print "1";;  
        Endif
      Next
      Print
    Next
  Else
    Print "El numero no es positivo"
  Endif
End
