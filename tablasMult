' Gambas module file

Public Sub Main()
  Dim i, j, n As Byte
  
  Print "Tablas de multiplicar"
  Print "Introduzca un numero entero positivo"
  Input n
  
  If n > 0 Then
    For i = 1 To n Step 1 
      For j = 1 To 10 Step 1
        Print i; " * "; j; " = "; i * j
      Next
      Print 
    Next      
  Else 
    Print "Numero no positivo"
  Endif
  
End
