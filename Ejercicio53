' Gambas module file

Public Sub Main()
  Dim i, j, m, n As Short
  
  Print "Ingrese dos numeros positivos"
  Input m, n
  
  If m > 0 And n > 0 Then
    For i = 1 To m Step 1 'Renglones
      For j = 1 To n Step 1 'Columnas
        If j == 1 Then
          Print i,
        Else
          Print j ^ i,
        Endif
      Next
      Print
    Next
  Else 
    Print "Los numeros deben ser positivos"
  Endif
End
